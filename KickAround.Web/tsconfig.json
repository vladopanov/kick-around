{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "types": [ "webpack-env" ],
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": [
      "es6",
      "dom"
    ]
  },
  "exclude": [
    "bin",
    "node_modules"
  ]
}
